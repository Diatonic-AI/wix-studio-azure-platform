{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION=eastus}"
    },
    "resourceBaseName": {
      "value": "wixagency"
    },
    "sqlAdminPassword": {
      "value": "${SQL_ADMIN_PASSWORD}"
    },
    "microservicesImage": {
      "value": "${MICROSERVICES_IMAGE=ghcr.io/diatonic-ai/wix-studio-azure-platform/microservices:latest}"
    },
    "githubToken": {
      "value": "${GITHUB_TOKEN=}"
    }
  }
}
